<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
              http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
              version="2.3">

    <navigation-rule>
        <from-view-id>Admin.xhtml</from-view-id>
        <navigation-case>
            <from-action>Admin</from-action>
            <from-outcome>goToAdmin</from-outcome>
            <to-view-id>/panel/AdminPanel/Admin.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
        <navigation-case>
            <from-action>Hayvanlar</from-action>
            <from-outcome>goToHayvanlar</from-outcome>
            <to-view-id>/panel/AdminPanel/Hayvanlar/Hayvanlar.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
        <navigation-case>
            <from-action>Ürünler</from-action>
            <from-outcome>goToÜrünler</from-outcome>
            <to-view-id>/panel/AdminPanel/Ürünler/Ürünler.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
        <navigation-case>
            <from-action>Kategori</from-action>
            <from-outcome>goToKategori</from-outcome>
            <to-view-id>/panel/AdminPanel/Kategori/kategori.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
    </navigation-rule>
</faces-config>